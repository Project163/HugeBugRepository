                <updated>Wed, 14 Dec 2011 08:40:32 +0000</updated>
                            <resolved>Sun, 4 Dec 2011 00:09:37 +0000</resolved>
                                    <version>3.2</version>
                                    <fixVersion>3.2</fixVersion>
