&lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; &lt;span class=&quot;code-keyword&quot;&gt;interface&lt;/span&gt; MyModel {

    @Inject
    &lt;span class=&quot;code-keyword&quot;&gt;private&lt;/span&gt; ImageModel image;
}
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;

&lt;p&gt;Does not.&lt;/p&gt;</description>
