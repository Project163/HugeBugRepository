+
+        LanguageProfile foobar = new LanguageProfile();
+        assertTrue(foo.distance(foobar) == bar.distance(foobar));
+
+        foobar.add("foo");
+        assertTrue( foo.distance(foobar) < bar.distance(foobar));
+
+        foobar.add("bar");
+        assertTrue(foo.distance(foobar) == bar.distance(foobar));
+    }
