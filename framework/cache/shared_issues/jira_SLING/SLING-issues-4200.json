                            <attachment id="12416305" name="SLING-1069-test.patch" size="5375" author="bdelacretaz" created="Wed, 12 Aug 2009 12:02:06 +0000"/>
                            <attachment id="12415469" name="SLING-1069.patch" size="1636" author="vramdal" created="Tue, 4 Aug 2009 08:31:24 +0000"/>
