                                    <reporter username="bazzargh">Brian Ewins</reporter>
                        <labels>
                    </labels>
                <created>Thu, 17 Jul 2003 22:14:54 +0000</created>
                <updated>Thu, 4 Sep 2003 23:36:11 +0000</updated>
                            <resolved>Thu, 4 Sep 2003 23:36:11 +0000</resolved>
                                                                    <component>core / taglib.core</component>
                        <due></due>
                            <votes>0</votes>
                                    <watches>0</watches>
