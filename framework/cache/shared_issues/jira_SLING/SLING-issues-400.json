}

@Override
&lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; void createAce(&lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt; principalId, &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;[] grantedPrivilegeNames, &lt;span class=&quot;code-object&quot;&gt;String&lt;/span&gt;[] deniedPrivilegeNames,
