Extension=css;
resourcePath=/bin/querybuilder.json
selectors=[]
seclectorString=&lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;
suffix=/..;/bin/querybuilder.json.css
 &lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
