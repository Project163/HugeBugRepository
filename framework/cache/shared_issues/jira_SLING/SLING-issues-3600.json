                                    <reporter username="mrpotes">James Phillpotts</reporter>
                        <labels>
                    </labels>
                <created>Thu, 21 Jun 2012 12:35:37 +0000</created>
                <updated>Wed, 20 Sep 2017 12:22:53 +0000</updated>
                            <resolved>Tue, 26 Jun 2012 14:24:04 +0000</resolved>
                                                    <fixVersion>Framework Extension Fragment WS 1.0.0</fixVersion>
                    <fixVersion>Framework Extension Fragment XML 1.0.0</fixVersion>
                    <fixVersion>Framework Extension Fragment Transaction 1.0.0</fixVersion>
                    <fixVersion>Framework Extension Fragment Activation 1.0.0</fixVersion>
                                    <component>Extensions</component>
                        <due></due>
                            <votes>0</votes>
                                    <watches>2</watches>
