                <updated>Mon, 15 Dec 2014 14:26:32 +0000</updated>
                            <resolved>Tue, 25 Nov 2014 17:36:45 +0000</resolved>
                                    <version>JUnit Remote Tests Runners 1.0.8</version>
                                    <fixVersion>JUnit Core 1.0.10</fixVersion>
                                    <component>Testing</component>
