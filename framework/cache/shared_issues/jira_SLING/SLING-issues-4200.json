                <updated>Fri, 16 Jan 2009 20:12:44 +0000</updated>
                            <resolved>Fri, 16 Jan 2009 07:33:58 +0000</resolved>
                                    <version>JCR OCM 2.0.4</version>
                                    <fixVersion>JCR OCM 2.0.4</fixVersion>
                                    <component>JCR</component>
                        <due></due>
                            <votes>0</votes>
                                    <watches>0</watches>
