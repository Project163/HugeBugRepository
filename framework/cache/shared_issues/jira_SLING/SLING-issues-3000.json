                <updated>Wed, 20 Sep 2017 12:08:48 +0000</updated>
                            <resolved>Wed, 25 Jun 2014 21:54:54 +0000</resolved>
                                    <version>Scripting JSP-Taglib 2.2.0</version>
                                    <fixVersion>Scripting JSP-Taglib 2.2.2</fixVersion>
                                    <component>Scripting</component>
                        <due></due>
                            <votes>0</votes>
                                    <watches>1</watches>
