                                        <assignee username="bdelacretaz">Bertrand Delacretaz</assignee>
                                    <reporter username="radu">Radu Cotescu</reporter>
                        <labels>
                    </labels>
                <created>Tue, 21 Oct 2014 07:46:29 +0000</created>
                <updated>Mon, 12 Jan 2015 13:00:03 +0000</updated>
                            <resolved>Tue, 21 Oct 2014 09:36:18 +0000</resolved>
