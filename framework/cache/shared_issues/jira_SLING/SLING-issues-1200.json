&lt;p&gt;     private void processFeature(Feature feature, Extension extension) {&lt;/p&gt;
&lt;ul class=&quot;alternate&quot; type=&quot;square&quot;&gt;
	&lt;li&gt;if (feature == null) {&lt;br/&gt;
+        if (feature == null || extension == null) 
{
             return;
         }
&lt;p&gt;         String defaultOrder = feature.getVariables().get(DEFAULT_CONTENT_START_ORDER);&lt;/p&gt;&lt;/li&gt;
&lt;/ul&gt;





