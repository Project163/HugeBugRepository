                                        <assignee username="-1">Unassigned</assignee>
                                    <reporter username="pallo">Markus Pallo</reporter>
                        <labels>
                    </labels>
                <created>Thu, 30 Jul 2009 14:21:15 +0000</created>
                <updated>Thu, 13 Apr 2023 04:58:05 +0000</updated>
                            <resolved>Thu, 13 Apr 2023 04:58:05 +0000</resolved>
                                    <version>JCR OCM 2.0.4</version>
                                                    <component>JCR</component>
                        <due></due>
                            <votes>0</votes>
                                    <watches>0</watches>
                                                                                                                <comments>
                            <comment id="12737113" author="pallo" created="Thu, 30 Jul 2009 14:23:12 +0000"  >&lt;p&gt;patchfile for ClassDescriptor reader to fix all three issues&lt;/p&gt;</comment>
                    </comments>
