                                    <reporter username="rombert">Robert Munteanu</reporter>
                        <labels>
                    </labels>
                <created>Wed, 3 Sep 2014 12:13:20 +0000</created>
                <updated>Wed, 3 Sep 2014 13:00:27 +0000</updated>
                            <resolved>Wed, 3 Sep 2014 13:00:27 +0000</resolved>
                                    <version>JUnit Remote Tests Runners 1.0.8</version>
                                    <fixVersion>JUnit Remote Tests Runners 1.0.10</fixVersion>
