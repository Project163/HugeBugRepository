                <updated>Wed, 18 Feb 2009 14:36:33 +0000</updated>
                            <resolved>Wed, 18 Feb 2009 14:36:33 +0000</resolved>
                                    <version>JCR Jackrabbit Server 2.0.4</version>
                                    <fixVersion>JCR Jackrabbit Server 2.0.4</fixVersion>
                                    <component>JCR</component>
