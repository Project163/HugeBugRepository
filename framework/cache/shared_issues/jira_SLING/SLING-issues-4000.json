                                    <reporter username="gaeremyncks">Simon Gaeremynck</reporter>
                        <labels>
                    </labels>
                <created>Wed, 24 Feb 2010 12:39:26 +0000</created>
                <updated>Fri, 10 Sep 2010 13:13:23 +0000</updated>
