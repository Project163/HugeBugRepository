                                    <reporter username="rafael.marins">Rafael Marins</reporter>
                        <labels>
                    </labels>
                <created>Tue, 24 Nov 2009 23:39:45 +0000</created>
                <updated>Thu, 2 Sep 2010 13:48:08 +0000</updated>
                            <resolved>Thu, 2 Sep 2010 13:48:08 +0000</resolved>
                                    <version>JCR Jackrabbit Server 2.0.6</version>
