                 String paramTypesAttr = attributes.getValue(&quot;paramtypes&quot;);&lt;br/&gt;
                 if (paramTypesAttr == null || paramTypesAttr.length() == 0) &lt;/p&gt;
{
                     callMethodRule = new CallMethodRule(targetDigester, 
methodName,
