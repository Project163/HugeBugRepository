                                    <reporter username="jshowlett">Scott Howlett</reporter>
                        <labels>
                    </labels>
                <created>Mon, 6 Oct 2003 18:32:59 +0000</created>
                <updated>Thu, 12 Aug 2004 07:36:44 +0000</updated>
                            <resolved>Thu, 12 Aug 2004 07:36:44 +0000</resolved>
                                                    <fixVersion>1.0-beta-4</fixVersion>
