                <created>Mon, 25 Jan 2010 23:08:03 +0000</created>
                <updated>Sun, 17 Jun 2012 09:04:26 +0000</updated>
                            <resolved>Tue, 26 Jan 2010 11:31:50 +0000</resolved>
                                    <version>Servlets Get 2.0.8</version>
                    <version>JCR Resource 2.0.6</version>
                                    <fixVersion>Servlets Get 2.1.0</fixVersion>
                    <fixVersion>JCR Resource 2.0.8</fixVersion>
