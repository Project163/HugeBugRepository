paramcount=&quot;0&quot; /&amp;gt;   &lt;br/&gt;
   	&amp;lt;call-method-rule pattern=&quot;author&quot; methodname=&quot;setAuthor&quot; &lt;br/&gt;
paramcount=&quot;0&quot; /&amp;gt;  &lt;/p&gt;

&lt;p&gt;   	&amp;lt;call-method-rule pattern=&quot;id&quot; methodname=&quot;setId&quot; paramcount=&quot;0&quot; /&amp;gt;  &lt;br/&gt;
   	&amp;lt;call-method-rule pattern=&quot;price&quot; methodname=&quot;setPrice&quot; &lt;br/&gt;
paramcount=&quot;0&quot; /&amp;gt;  &lt;br/&gt;
   	&amp;lt;call-method-rule pattern=&quot;date&quot; methodname=&quot;setDate&quot; &lt;br/&gt;
paramcount=&quot;0&quot; /&amp;gt;  &lt;/p&gt;




&lt;p&gt;   	&amp;lt;pattern value=&quot;article&quot;&amp;gt;&lt;br/&gt;
        	&amp;lt;object-create-rule &lt;br/&gt;
classname=&quot;pro.prepreuve.digester.Article&quot; /&amp;gt;&lt;/p&gt;

&lt;p&gt;         		&amp;lt;set-properties-rule&amp;gt;&lt;br/&gt;
