import org.apache.sling.api.resource.LoginException;&lt;br/&gt;
@@ -78,6 +79,7 @@ public class TenantProviderImpl implements TenantProvider {&lt;br/&gt;
     @Property(&lt;br/&gt;
             value = {},&lt;br/&gt;
+            unbounded = PropertyUnbounded.ARRAY,&lt;br/&gt;
