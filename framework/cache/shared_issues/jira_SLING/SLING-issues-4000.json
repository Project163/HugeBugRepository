esayfasi&quot;,&quot;slogan&quot;:&quot;Jelmondat&quot;,&quot;jcr:mixinTypes&quot;:[&quot;rep:AccessControllable&quot;],&quot;jcr:primaryType&quot;:&quot;esayfasi:TemplatedSite&quot;}

&lt;p&gt;4. curl &quot;http://localhost:8888/.query.json?queryType=xpath&amp;amp;statement=//element(*,esayfasi:TemplatedSite)&quot;&lt;br/&gt;
[{&quot;name&quot;:&quot;test1.semmi.se&quot;,&quot;slogan&quot;:&quot;Jelmondat&quot;,&quot;contactName&quot;:&quot;Robert Csakany&quot;,&quot;jcr:score&quot;:6373,&quot;googleApiKey&quot;:&quot;google_api_key_semmi.se&quot;,&quot;owner&quot;:&quot;esayf&lt;br/&gt;
asi&quot;,&quot;template&quot;:&quot;8899&quot;,&quot;jcr:primaryType&quot;:&quot;esayfasi:TemplatedSite&quot;,&quot;email&quot;:&quot;robson@semmi.se&quot;,&quot;jcr:path&quot;:&quot;/sites/test1.semmi.se&quot;,&quot;headline&quot;:&quot;Fejl&#915;&#234;&#220;&#9516;&#8976;c&quot;&lt;br/&gt;
