                                    <reporter username="radu">Radu Cotescu</reporter>
                        <labels>
                    </labels>
                <created>Wed, 26 Aug 2020 15:19:12 +0000</created>
                <updated>Thu, 1 Oct 2020 19:38:48 +0000</updated>
                            <resolved>Thu, 27 Aug 2020 10:33:03 +0000</resolved>
                                    <version>Scripting Sightly Engine 1.0.0</version>
